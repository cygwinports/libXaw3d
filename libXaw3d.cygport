inherit xorg

NAME="libXaw3d"
VERSION=1.6.2
RELEASE=2
CATEGORY="Libs"
SUMMARY="3-D Athena widget library"
DESCRIPTION="Three-dimensional Athena widget library"

PKG_NAMES="libXaw3d8 libXaw3d-devel"
libXaw3d8_CONTENTS="usr/bin/cygXaw3d-8.dll usr/share/doc/"
libXaw3d_devel_CONTENTS="usr/include/ usr/lib/"

CYGCONF_ARGS="
	--enable-internationalization
	--enable-multiplane-bitmaps
	--enable-gray-stipples
	--enable-arrow-scrollbars
"

KEEP_LA_FILES="none"
